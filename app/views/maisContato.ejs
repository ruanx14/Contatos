<div class="viewmore">
    <p><strong>Nome</strong>: <%=contato.nome%></p>
    <p><strong>Sobrenome</strong>: <%=contato.sobrenome%></p>
    <p><strong>Idade</strong>: <%=contato.idade%></p>
    <p><strong>Email</strong>: <%=contato.email%></p>
    <p><strong>Cidade</strong>: <%=contato.cidade%></p>
    <% if(Array.isArray(contato.redes)){%>
        <% for(var i=0;i<contato.redes.length;i++){ %>
            <p><strong>Redes</strong>: <%=contato.redes[i]%></p>
        <% } %>
    <% } else { %>
        <p><strong>Redes</strong>: <%=contato.redes%></p>
    <% } %>
</div>
<div class="viewmore">
